			"metals_alloy4"	: [ "Tritanium Alloy",	"^orange;Tier 5^reset;\n\nLearn to process Tritanium Alloy." ],
			"metals_alloy5"	: [ "Precursor Alloy",	"^orange;Tier 5^reset;\n\nLearn to process Precursor Alloy." ],
